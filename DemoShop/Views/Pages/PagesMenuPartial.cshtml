@model IEnumerable<DemoShop.Models.ViewModels.Pages.PageVM>

@{ 
    string tempPageTitle = (string)TempData["PageTitle"];
}


@foreach (var item in Model) {
    if (tempPageTitle == item.Title)
    {
        <li class="active">
            <a href="/@item.Slug.ToLower()">@item.Title</a>
        </li>
    }
    else
    {
        <li>
            <a href="/@item.Slug.ToLower()">@item.Title</a>
        </li>
    }

}

